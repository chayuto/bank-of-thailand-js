{
  "name": "bank-of-thailand",
  "version": "1.0.1",
  "description": "TypeScript/JavaScript client for Bank of Thailand API - Works with Node.js, Bun, and browsers",
  "keywords": [
    "bank-of-thailand",
    "bot",
    "thailand",
    "exchange-rate",
    "api",
    "typescript",
    "bun",
    "nodejs"
  ],
  "license": "MIT",
  "author": "Chayut",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chayuto/bank-of-thailand-js.git"
  },
  "homepage": "https://github.com/chayuto/bank-of-thailand-js#readme",
  "bugs": {
    "url": "https://github.com/chayuto/bank-of-thailand-js/issues"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.ts",
        "require": "./dist/index.d.cts"
      },
      "bun": "./dist/index.js",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": ["dist", "README.md", "CHANGELOG.md", "LICENSE"],
  "engines": {
    "node": ">=20.0.0",
    "bun": ">=1.3.0"
  },
  "packageManager": "bun@1.3.0",
  "scripts": {
    "build": "tsup",
    "build:check": "publint",
    "typecheck:attw": "attw --pack .",
    "test": "bun test",
    "test:node": "vitest run",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "bench": "bun test --run bench/*.bench.ts",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit",
    "dev": "tsup --watch",
    "prepublishOnly": "bun run build && bun test && bun run lint && bun run build:check",
    "docs": "typedoc",
    "docs:dev": "vitepress dev docs",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "bun run build && changeset publish"
  },
  "dependencies": {},
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.0",
    "@biomejs/biome": "^1.9.4",
    "@changesets/cli": "^2.27.11",
    "@types/bun": "latest",
    "@types/node": "^22.10.2",
    "@vitest/coverage-v8": "^2.1.8",
    "msw": "^2.7.0",
    "publint": "^0.2.12",
    "simple-git-hooks": "^2.12.1",
    "tsup": "^8.3.5",
    "typedoc": "^0.26.11",
    "typescript": "^5.7.2",
    "vitepress": "^1.5.0",
    "vitest": "^2.1.8"
  },
  "simple-git-hooks": {
    "pre-commit": "bun lint-staged"
  },
  "lint-staged": {
    "*.{ts,js,json}": ["biome check --write"]
  },
  "trustedDependencies": []
}
